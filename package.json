{
  "name": "eslint-config-liting",
  "version": "0.0.0",
  "description": "eslint preset of liting",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "format": "eslint --fix --color --ext .js",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it --changelog"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-standard/eslint-config-liting.git"
  },
  "homepage": "https://github.com/dev-standard/eslint-config-liting#readme",
  "author": "liting-yes <luz.liting@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "dotenv-cli": "^6.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "release-it": "^15.5.1"
  },
  "dependencies": {
    "eslint": "^8.29.0"
  }
}