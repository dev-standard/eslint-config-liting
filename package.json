{
  "name": "eslint-config-liting",
  "version": "0.3.0",
  "description": "eslint preset of liting",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "format": "eslint --fix --color --ext .js --ignore-pattern /__tests__/",
    "release": "release-it",
    "build": "unbuild",
    "commit": "git-cz"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-standard/eslint-config-liting.git"
  },
  "homepage": "https://github.com/dev-standard/eslint-config-liting#readme",
  "author": "liting-yes <luz.liting@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/cz-commitlint": "^17.3.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "commitizen": "^4.2.6",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "release-it": "^15.5.1",
    "unbuild": "^1.0.2",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vue-eslint-parser": "^9.1.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}